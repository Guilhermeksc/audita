[
    {
      "id": "deploy",
      "execute-command": "/deploy.sh",
      "command-working-directory": "/",
      "response-message": "Deploy iniciado.",
      "trigger-rule": {
        "match": {
          "type": "payload-hash-sha256",
          "secret": "${WEBHOOK_SECRET}",
          "parameter": {
            "source": "header",
            "name": "X-Hub-Signature-256"
          }
        }
      }
    }
  ]
  